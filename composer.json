{
  "name": "fostercommerce/craft-foster-checkout",
  "description": "A companion plugin to the Foster Checkout Commerce templates.",
  "type": "craft-plugin",
  "license": "mit",
  "version": "1.1.0",
  "support": {
    "email": "support@fostercommerce.com"
  },
  "require": {
    "php": ">=8.0.2",
    "craftcms/cms": "^4.4.6"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "fostercommerce\\craftfostercheckout\\": "src/"
    }
  },
  "extra": {
    "handle": "foster-checkout",
    "name": "Foster Checkout",
    "developer": "Foster Commerce",
    "documentationUrl": "",
    "class": "fostercommerce\\craftfostercheckout\\FosterCheckout"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "phpstan": "phpstan --memory-limit=1G",
    "ecs-check": "ecs check --ansi --memory-limit=1G",
    "ecs-fix": "ecs check --ansi --fix --memory-limit=1G",
    "rector": "rector process",
    "rector-dry-run": "rector process --dry-run"
  }
}
